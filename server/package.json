{
    "name": "jobika-server",
    "version": "1.0.0",
    "description": "JoBika Backend API",
    "main": "index.js",
    "scripts": {
        "start": "node start.js",
        "dev": "nodemon index.js",
        "test": "jest --setupFilesAfterEnv=./tests/setup.js",
        "test:watch": "jest --watch --setupFilesAfterEnv=./tests/setup.js",
        "test:coverage": "jest --coverage --setupFilesAfterEnv=./tests/setup.js",
        "migrate": "sequelize-cli db:migrate",
        "migrate:undo": "sequelize-cli db:migrate:undo",
        "db:sync": "node scripts/syncDatabase.js",
        "seed": "node scripts/seed.js",
        "generate:jobs": "node scripts/generateJobs.js",
        "backup": "node scripts/backup.js create",
        "backup:list": "node scripts/backup.js list",
        "backup:restore": "node scripts/backup.js restore",
        "db:stats": "node scripts/dbStats.js"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cheerio": "^1.0.0-rc.12",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "mammoth": "^1.6.0",
        "multer": "^1.4.5-lts.1",
        "natural": "^6.10.3",
        "node-cron": "^3.0.3",
        "nodemailer": "^6.9.7",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-linkedin-oauth2": "^2.0.0",
        "pdf-parse": "^1.1.1",
        "pg": "^8.16.3",
        "pg-hstore": "^2.3.4",
        "qrcode": "^1.5.3",
        "sequelize": "^6.35.2",
        "speakeasy": "^2.0.0"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "sequelize-cli": "^6.6.2",
        "sqlite3": "^5.1.7",
        "supertest": "^6.3.3"
    }
}